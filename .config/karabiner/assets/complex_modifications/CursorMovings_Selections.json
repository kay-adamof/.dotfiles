          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
                "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "x",
              "modifiers": ["left_command"]
            }
          ]
        },
        {

    {
      "description": "copy_paste",
      "manipulators": [
        {
          "description": "copy",
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
                "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "description": "paste",
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
                "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
